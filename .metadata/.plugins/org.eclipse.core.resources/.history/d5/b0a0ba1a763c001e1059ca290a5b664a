package leetCodeEx;

public class SumTwo {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
			
		
		
	}
	
	class Solution {
	    public int[] twoSum(int[] nums, int target) {
	        for(int i= 0; i < nums.lenght; i++){
	            if((nums[i] + nums[i + 1]) = target){
	            system.out.println(nums[i], nums[i +1]);
	            
	            }
	        }
	        
	    }
	}

}
